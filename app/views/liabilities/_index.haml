%h2 Your Previous Liabilities
%br
- outp = []
- liabilities.each do |i|
  - if i.paid_on?
    - @pup = "Paid"
  - else
    - @pup = "Unpaid"
  - outp << [link_to_remote(i.contact.name_long, :url => {:action => :view, :id => i.id}), i.incurred_on.inspect, @pup]
- outp << [do_pag(liabilities), ""]
- if liabilities.size == 0
  Sorry, no results.
- else
  = gen_list outp, {:head => ['Supplier', 'Incurred On', 'Paid?'], :size => 3}
